{
    "name": "tunasahin/seo-images",
    "description": "Laravel SEO uyumlu görsel yönetim paketi - otomatik JPG, WebP ve AVIF formatları",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Tuna Sahin",
        "email": "tuna@example.com"
    }],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/filesystem": "^10.0|^11.0",
        "intervention/image": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "TunaSahin\\SeoImages\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TunaSahin\\SeoImages\\SeoImagesServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
